   "source": [
    "def Factorial(x):\n",
    "    if x<=0:\n",
    "        return 1\n",
    "    else:\n",
    "        return (x * Factorial(x-1))\n",
    "\n",
    "# Calling Function \n",
    "\n",
    "x = 5\n",
    "print(Factorial(x))"
   ]
  }
